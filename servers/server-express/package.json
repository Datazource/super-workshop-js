{
  "name": "super-workshop-js",
  "version": "1.0.0",
  "description": "Super Workshop JS - Server Express",
  "main": "server.js",
  "scripts": {
    "start:dev": "pm2 startOrGracefulReload ecosystem.json --env dev",
    "start:staging": "pm2 startOrGracefulReload ecosystem.json --env staging",
    "start": "pm2 startOrGracefulReload ecosystem.json --env production",
    "debug": "nodemon server.js",
    "stop": "pm2 stop all",
    "delete": "pm2 delete all",
    "push": "git push origin && git push origin --tags",
    "release:major": "npm version major && npm run push",
    "release:minor": "npm version minor && npm run push",
    "release:patch": "npm version patch && npm run push",
    "deploy:dev:setup": "NODE_ENV=dev pm2 deploy ecosystem.json dev setup",
    "deploy:dev": "NODE_ENV=dev pm2 deploy ecosystem.json dev",
    "deploy:staging:setup": "NODE_ENV=staging pm2 deploy ecosystem.json staging setup",
    "deploy:staging": "NODE_ENV=staging pm2 deploy ecosystem.json staging",
    "deploy:setup": "NODE_ENV=production pm2 deploy ecosystem.json production setup",
    "deploy": "NODE_ENV=production pm2 deploy ecosystem.json production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhaidarh/super-workshop-js.git"
  },
  "keywords": [
    "express",
    "workshop"
  ],
  "author": "M Haidar Hanif <npm@mhaidarhanif.com> (https://mhaidarhanif.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mhaidarh/super-workshop-js/issues"
  },
  "homepage": "https://github.com/mhaidarh/super-workshop-js#readme",
  "dependencies": {
    "acho": "^3.1.1",
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "express-jwt": "^5.1.0",
    "express-jwt-permissions": "^0.5.0",
    "express-session": "^1.14.2",
    "express-validator": "^2.21.0",
    "jsonwebtoken": "^7.1.9",
    "mongoose": "^4.6.8",
    "mongoose-paginate": "^5.0.3",
    "mongoose-sequence": "^3.1.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth": "^1.0.0",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "passport-twitter": "^1.0.4",
    "ramda": "0.22.1",
    "request-ip": "1.2.3"
  },
  "devDependencies": {
    "chance-cli": "^1.0.6",
    "mocha": "^3.1.2",
    "npm-check-updates": "2.8.6",
    "pm2": "^2.1.5",
    "standard": "^8.5.0"
  }
}
